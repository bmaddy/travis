<%form_for (@story) do |f| %>
<fieldset>
<%=f.label "Title" %>
<%=f.text_field :title %>
<br class="clear-left" />
<%=f.label "Description" %>
<%=f.text_area :description%>
<br class="clear-left" />
<%=f.label "Swag" %>
<%=f.text_field :swag%>
<br class="clear-left" />
</fieldset>
<%=submit_tag "Save", {:id=> "submit_button"} %>
<%=button_to_function "Cancel", "location.href='#{stories_path}'" %>
<% end %>
